{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "brace-style": ["error", "stroustrup"],
        "class-methods-use-this": ["off"],
        "comma-dangle": ["error", "never"],
        "indent": ["error", 4],
        "linebreak-style": ["off"],
        "max-len": ["warn", 160],
        "no-console": ["off"],
        "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["res"] }],
        "object-shorthand": ["error", "consistent-as-needed"]
    }
}
